{
  "name": "tella-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3000",
    "dev:fast": "cross-env TURBOPACK=1 next dev --turbopack --port 3000",
    "dev:debug": "cross-env NODE_OPTIONS=--inspect next dev --turbopack",
    "build": "next build && next-sitemap",
    "build:turbo": "cross-env TURBOPACK=1 next build --turbopack && next-sitemap",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:turbo": "next start --turbopack",
    "lint": "next lint --turbopack",
    "lint:fix": "next lint --fix --turbopack",
    "type-check": "tsc --noEmit --incremental",
    "sitemap": "next-sitemap",
    "clean": "rimraf .next .turbo out node_modules/.cache",
    "clean:turbo": "rimraf .next .turbo node_modules/.cache",
    "preview": "npm run build && npm run start",
    "check-turbo": "node scripts/check-turbopack.js",
    "optimize": "npm run clean:turbo && npm run build:turbo"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "next": "15.5.2",
    "next-sitemap": "^4.2.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
